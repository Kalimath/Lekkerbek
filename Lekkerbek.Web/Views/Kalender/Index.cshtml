@model IEnumerable<Lekkerbek.Web.ViewModels.Kalender.KalenderIndexViewModel>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h4>Tijdsloten</h4>
<table class="table">
    <thead>
        <tr>
            <th>
                Tijdstip
            </th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model.First().Momenten)
        {
            <tr>
                <td><a asp-action="Details" asp-route-id="item">@Html.DisplayNameFor(item => item)</a></td>
            </tr>
        }

    </tbody>
</table>
